{
  "connection": "default",
  "collectionName": "students",
  "info": {
    "name": "Student"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "college": {
      "via": "students",
      "model": "college"
    },
    "user": {
      "unique": true,
      "plugin": "users-permissions",
      "model": "user"
    },
    "Stream": {
      "type": "string"
    },
    "documents": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "verifiedByCollege": {
      "type": "boolean"
    },
    "physicallyHandicapped": {
      "type": "boolean"
    }
  }
}
