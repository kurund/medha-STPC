{
  "connection": "default",
  "collectionName": "activities",
  "info": {
    "name": "Activity"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "activity_type": {
      "type": "enumeration",
      "enum": [
        "workshop",
        "industrialVisit",
        "training"
      ]
    },
    "academic_year": {
      "model": "academic-year"
    },
    "college": {
      "model": "college"
    },
    "start_datetime": {
      "type": "datetime"
    },
    "end_datetime": {
      "type": "datetime"
    },
    "education_year": {
      "type": "enumeration",
      "enum": [
        "First",
        "Second",
        "Third",
        "Fourth"
      ]
    },
    "streams": {
      "collection": "stream"
    }
  }
}
